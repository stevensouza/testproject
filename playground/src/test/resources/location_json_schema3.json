{
  "definitions": {
          "address": {
          "type": "object",
          "title": "The Address Schema",
          "required": [
            "addressId",
            "city",
            "state",
            "zip",
            "county",
            "addr1"
          ],
          "properties": {
            "addressId": {
              "type": "integer",
              "title": "The Addressid Schema",
              "default": 0,
              "examples": [
                1000
              ]
            },
            "city": {
              "type": "string",
              "title": "The City Schema",
              "default": "",
              "examples": [
                "Anytown"
              ],
              "pattern": "^(.*)$"
            },
            "state": {
              "type": "string",
              "title": "The State Schema",
              "default": "",
              "examples": [
                "VA"
              ],
              "enum": ["NY","VA"]
            },
            "zip": {
              "type": "string",
              "title": "The Zip Schema",
              "default": "",
              "examples": [
                "22031"
              ],
              "pattern": "^[0-9]{5}$"
            },
            "zip4": {
              "type": ["string","null"],
              "title": "The Zip4 Schema",
              "default": null,
              "examples": [
                null
